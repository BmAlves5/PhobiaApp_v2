<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <ImageView
        android:id="@+id/imageViewBackground"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="-700dp"
        android:layout_marginRight="-5dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:foregroundTint="#FFFFFF"
        android:src="@drawable/retangulo_inicialat" />

    <View
        android:id="@+id/image_head"
        android:layout_width="123dp"
        android:layout_height="127dp"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/head" />

    <TextView
        android:id="@+id/phobia_app"
        android:layout_width="171dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="40dp"
        android:gravity="center_horizontal|top"
        android:hint="Phobia App"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColorHint="#FFFFFFFF"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/Test"
        android:layout_width="171dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="95dp"
        android:gravity="center_horizontal|top"
        android:hint="Test"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColorHint="#FFFFFFFF"
        android:textSize="35dp"
        android:textStyle="bold" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/statistics"
            android:layout_width="68dp"
            android:layout_height="75dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="311dp"
            android:background="@drawable/statistics"
            android:elevation="10dp" />

        <Button
            android:id="@+id/statisticsButton"
            android:layout_width="300dp"
            android:layout_height="100dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="300dp"
            android:backgroundTint="#E8E4E4"
            android:paddingRight="-40dp"
            android:text="Statistics"
            android:textSize="20dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/patientfeedback"
            android:layout_width="68dp"
            android:layout_height="75dp"
            android:layout_marginLeft="70dp"
            android:layout_marginTop="435dp"
            android:background="@drawable/patientfeedback"
            android:elevation="10dp" />

        <Button
            android:id="@+id/patientfeedbackButton"
            android:layout_width="300dp"
            android:layout_height="100dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="420dp"
            android:backgroundTint="#E8E4E4"
            android:paddingRight="-40dp"
            android:text="Patient Feedback"
            android:textSize="20dp" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:backgroundTint="#FFFFFF">


        <ImageView
            android:id="@+id/base"
            android:layout_width="367dp"
            android:layout_height="113dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="690dp"
            android:background="@drawable/rectanglegrey" />

        <Button
            android:id="@+id/accountButton"
            android:layout_width="47dp"
            android:layout_height="48dp"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="723dp"
            android:background="@drawable/accountgrey"
            android:elevation="10dp"
            android:gravity="center" />

        <TextView
            android:id="@+id/account"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="120dp"
            android:layout_marginTop="767dp"
            android:elevation="10dp"
            android:gravity="center_horizontal|top"
            android:hint="Account"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textColor="#000000"
            android:textSize="15dp" />

        <Button
            android:id="@+id/homeButton"
            android:layout_width="36dp"
            android:layout_height="41dp"
            android:layout_marginLeft="92dp"
            android:layout_marginTop="725dp"
            android:background="@drawable/homegrey"
            android:elevation="10dp"
            android:gravity="center" />

        <TextView
            android:id="@+id/home"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="768dp"
            android:elevation="10dp"
            android:gravity="center_horizontal|top"
            android:hint="Home"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textColor="#000000"
            android:textSize="15dp" />

        <Button
            android:id="@+id/exitButton"
            android:layout_width="53dp"
            android:layout_height="50dp"
            android:layout_marginLeft="275dp"
            android:layout_marginTop="720dp"
            android:background="@drawable/exit"
            android:elevation="10dp"
            android:gravity="center" />

        <TextView
            android:id="@+id/exit"
            android:layout_width="171dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="215dp"
            android:layout_marginTop="768dp"
            android:elevation="10dp"
            android:gravity="center_horizontal|top"
            android:hint="Exit"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textColor="#000000"
            android:textSize="15dp" />


    </RelativeLayout>

</RelativeLayout>